{
  "name": "ruby-exchange-deployments",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@boringcrypto/boring-solidity": "boringcrypto/BoringSolidity#e06e943",
    "@codechecks/client": "^0.1.11",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@ethersproject/abi": "^5.4.1",
    "@ethersproject/abstract-signer": "^5.4.1",
    "@ethersproject/bignumber": "^5.4.1",
    "@ethersproject/bytes": "^5.4.0",
    "@ethersproject/providers": "^5.4.5",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.11",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^3.1.0",
    "@openzeppelin/contracts-upgradeable": "^3.1.0",
    "@openzeppelin/hardhat-upgrades": "^1.14.0",
    "@tenderly/hardhat-tenderly": "^1.0.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@uniswap/v2-core": "git://github.com/uniswap/uniswap-v2-core.git#27f6354bae6685612c182c3bc7577e61bc8717e3",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.2.0",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.2",
    "hardhat-abi-exporter": "^2.0.8",
    "hardhat-deploy": "^0.9.1",
    "hardhat-deploy-ethers": "^0.3.0-beta.11",
    "hardhat-gas-reporter": "^1.0.0",
    "hardhat-preprocessor": "^0.1.0",
    "hardhat-spdx-license-identifier": "^2.0.0",
    "hardhat-typechain": "^0.3.5",
    "hardhat-watcher": "^2.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "shelljs": "^0.8.4",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.2.2",
    "web3": "^1.6.0"
  },
  "scripts": {
    "chain": "hardhat node --network hardhat --no-deploy",
    "test": "hardhat test --network hardhat",
    "deploy": "hardhat deploy",
    "accounts": "hardhat accounts",
    "balance": "hardhat balance",
    "send": "hardhat send",
    "generate": "hardhat generate",
    "account": "hardhat account",
    "exportAbis": "hardhat export-abi",
    "etherscan-verify": "hardhat etherscan-verify --api-key ZG3GNW27H3216I9X5JGRXIJWX25CZDABFZ",
    "clean": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat clean",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "cross-env CODE_COVERAGE=true hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.ts\"",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 15 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "husky install",
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,sol,ts}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "initCodeHash": "hardhat run scripts/initCodeHash.ts",
    "transferRUBYtoMS": "hardhat run scripts/transferRUBYtoMS.ts",
    "debugFees": "hardhat run scripts/debugging/fees.ts",
    "registerTokensToIMA": "hardhat run scripts/bridging/registerTokensToIMA.ts",
    "bridgeTokens": "hardhat run scripts/bridging/bridgeTokens.ts",
    "createAMMLPs": "hardhat run scripts/seeding/createAMMLPs.ts",
    "createFarms": "hardhat run scripts/seeding/createFarms.ts",
    "createLottery": "hardhat createLottery",
    "drawLottery": "hardhat drawLottery",
    "ima": "hardhat run scripts/bridging/ima.ts",
    "seedFaucet": "hardhat run scripts/seeding/seedFaucet.ts",
    "multiChainBridge": "hardhat run scripts/bridging/multiSchainBridging.ts"
  },
  "files": [
    "/contracts"
  ],
  "lint-staged": {
    "**/*.sol": [
      "solhint --config ./.solhint.json --max-warnings 5 \"contracts/**/*.sol\""
    ],
    "**/*.{js,ts}": [
      "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts ."
    ],
    "*.{js,json,md,sol,ts,yaml,yml}": [
      "prettier --config ./.prettierrc.yaml --write"
    ]
  }
}
